@model SisalrilProject.Models.Asignacione

@{
    ViewData["Title"] = "Details";
}

<h1>Detalles</h1>

<div>
    <h4>Asignaciones</h4>
    <hr />
    <dl class="row">
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.AsignacionFechaInicio)
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.AsignacionFechaInicio)
        </dd>
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.AsignacionNoDias)
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.AsignacionNoDias)
