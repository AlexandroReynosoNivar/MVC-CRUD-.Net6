@model SisalrilProject.Models.Edificio

@{
    ViewData["Title"] = "Delete";
}

<h1>Borrar</h1>

<h3>¿Estás seguro de que quieres eliminar esto?</h3>
<div>
    <h4>Edificios</h4>
    <hr />
    <dl class="row">
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.EdificioDireccion)
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.EdificioDireccion)
        </dd>
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.TipoEdificio)
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.TipoEdificio)
        </dd>
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.NivelCalidad)
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.NivelCalidad)
        </dd>
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.Categoria)
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.Categoria)
        </dd>
    </dl>
    
    <form asp-action="Delete">
        <input type="hidden" asp-for="IdEdificio" />
        <input type="submit" value="Delete" class="btn btn-danger" /> |
        <a asp-action="Index"class="btn btn-primary">Volver a la lista</a>
    </form>
</div>
