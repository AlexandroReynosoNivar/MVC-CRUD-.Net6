        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.IdEdificioNavigation.IdEdificio)
        </dd>
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.IdTrabajadorNavigation)
        </dt>
        <dd class = "col-sm-10">
